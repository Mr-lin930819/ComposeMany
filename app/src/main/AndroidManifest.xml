<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mrlin.composemany">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".ComposeManyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/Theme.ComposeMany.NoActionBar">
        <activity
            android:name="com.mrlin.composemany.pages.music.NetEaseMusicSplashActivity"
            android:exported="true"
            android:label="@string/title_activity_net_ease_music" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/JetpackSplashTheme"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mrlin.composemany.pages.fund.FundActivity" />
    </application>

</manifest>